        "always"
      ],
      "indent": [
        "warn",
        4
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "semi": [
        "warn",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not chrome 49"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
